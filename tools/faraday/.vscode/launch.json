  "version": "0.2.0",
  "configurations": [
    {
      "name": "faraday tag",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "tag",
        "--project",
        "/Users/gix/Documents/yxr/g_faraday/integrate_example",
        "-v",
        "1.0.3+1234",
      ]
    },
    {
      "name": "faraday auto-impl",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "auto-impl",
        "-f",
        "../../example/lib/cli_demo/demo.dart"
      ]
    },
    {
      "name": "faraday generate",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "generate",
        "-f",
        "../../example/lib/cli_demo/demo.dart",
        "--ios-common",
        "../../example/ios/Runner/Faraday/FaradayCommon.swift",
        "--ios-route",
        "../../example/ios/Runner/Faraday/FaradayRoute.swift",
        "--android-route",
        "../../example/android/app/src/main/kotlin/com/yuxiaor/flutter/g_faraday_example/faraday/Route.kt",
        "--android-common",
        "../../example/android/app/src/main/kotlin/com/yuxiaor/flutter/g_faraday_example/faraday/Common.kt"
      ],
      "type": "dart"
    }
  ]
}