  "version": "0.2.0",
  "configurations": [
    {
      "name": "faraday tag",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "tag",
        "--project",
        "/Users/gix/Documents/yxr/faraday_reborn",
        "-v",
        "0.0.1+0"
      ]
    },
    {
      "name": "faraday completion",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "completion",
        "--offset",
        "1400",
        "--relative-path",
        "cli_demo/demo.dart",
        "--file",
        "../../example/lib/src/cli_demo/demo.dart"
      ]
    },
    {
      "name": "faraday generate",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "generate",
        "-f",
        "../../example/lib/cli_demo/demo.dart",
      ],
      "type": "dart"
    },
    {
      "name": "faraday init",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "init",
        "--project-path",
        "../../example"
      ],
      "type": "dart"
    }
  ]
}