  "version": "0.2.0",
  "configurations": [
    {
      "name": "faraday tag",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "tag",
        "--project",
        "/Users/gix/Documents/yxr/g_faraday/integrate_example",
        "-v",
        "1.0.3+1234",
      ]
    },
    {
      "name": "faraday auto-impl",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "auto-impl",
        "-f",
        "../../example/lib/cli_demo/demo.dart"
      ]
    },
    {
      "name": "faraday generate",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "generate",
        "-f",
        "../../example/lib/cli_demo/demo.dart",
      ],
      "type": "dart"
    },
    {
      "name": "faraday init module",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "init",
        "module",
        "--config-path",
        "../../.faraday_init.json"
      ],
      "type": "dart"
    },
    {
      "name": "faraday init native",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "init",
        "native",
        "--config-path",
        "../../.faraday_init.json"
      ],
      "type": "dart"
    }
  ]
}